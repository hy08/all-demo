{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@tmp/*": [
        "src/pages/.umi/*"
      ]
    },
    "allowSyntheticDefaultImports": true
  }
}